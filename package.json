{
  "name": "angular2-universal-starter",
  "version": "0.0.1",
  "description": "Angular 2 Universal starter",
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "karma": "karma",
    "protractor": "protractor",
    "pm2": "pm2",
    "nodemon": "nodemon",
    "clean": "npm cache clean && npm run rimraf -- node_modules typings coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "NODE_ENV=production webpack",
    "server": "npm run server:dev",
    "preserver:dev": "npm run build",
    "server:dev": "nodemon dist/server/server.js",
    "preserver:prod": "npm run build:prod",
    "server:prod": "NODE_ENV=production pm2 start ecosystem.json",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "lint": "npm run tslint 'src/**/*.ts'",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "pretest": "npm run lint",
    "test": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
    "ci": "npm run e2e && npm run test",
    "start": "npm run server:prod",
    "start:dev": "npm run server:dev",
    "restart": "npm stop && npm start",
    "stop": "pm2 delete ecosystem.json",
    "predebug": "npm run build",
    "debug": "node --debug-brk dist/server/server.js",
    "status": "pm2 show $(node -e \"console.log(require('./ecosystem.json').name)\")",
    "logs": "pm2 logs $(node -e \"console.log(require('./ecosystem.json').name)\")",
    "postinstall": "npm run typings -- install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "angular2-universal": "~0.98.3",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "morgan": "^1.7.0",
    "preboot": "^2.0.7",
    "rxjs": "5.0.0-beta.2"
  },
  "devDependencies": {
    "angular-cli": "0.0.34",
    "codelyzer": "0.0.18",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^2.1.3",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "lost": "^6.7.2",
    "nodemon": "^1.8.1",
    "pm2": "^1.1.3",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.8.2",
    "postcss-mixins": "^4.0.1",
    "postcss-nested": "^1.0.0",
    "postcss-partial-import": "^1.3.0",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.3",
    "ts-loader": "^0.8.1",
    "ts-node": "^0.7.2",
    "tslint": "^3.8.1",
    "typescript": "^1.8.10",
    "typings": "~0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.6",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.0.0-beta",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.12.0"
  }
}
